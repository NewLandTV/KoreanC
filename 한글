// 자료형
typedef void 빈;
typedef short 정수16;
typedef unsigned short 부호없는정수16;
typedef int 정수32;
typedef unsigned int 부호없는정수32;
typedef long long 정수64;
typedef unsigned long long 부호없는정수64;
typedef bool 불;
typedef float 실수;
typedef double 정밀실수;
typedef char 문자;
typedef unsigned char 부호없는문자;

