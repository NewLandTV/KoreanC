#define 중괄호열기 {
#define 중괄호닫기 }
#define 중괄호열기닫기 {}
#define 대괄호열기 [
#define 대괄호닫기 ]
#define 대괄호열기닫기 []

// 메인 함수
#define 진입점 main

// 참과 거짓
#define 참 true
#define 거짓 false

#define 상수 const

